{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react",
    "noImplicitAny": true,
    "noEmit": false
  }
}
