{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/ui.main.bundle.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build:bkup": "pnpm clean && webpack && tsup src/index.ts --format esm,cjs --dts --external react",
    "build": "pnpm clean && tsc ./src/*.ts --outDir ./dist/ && webpack && pnpm svgr",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest",
    "svgr": "npx @svgr/cli --out-dir ./dist/svgr/flashy -- ./src/card-decks/flashy"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.35.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.4.3",
    "jest-presets": "workspace:*",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.14.2",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
